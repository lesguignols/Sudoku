<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="550.0" prefWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="controller.ClassementMotusController">
   <children>
      <Text layoutX="140.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Classement du jeu MOTUS">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Label fx:id="text" alignment="CENTER" layoutY="94.0" prefHeight="17.0" prefWidth="750.0" text="Label" textFill="RED" />
      <TableView fx:id="tableauScoreMotus" layoutX="300.0" layoutY="161.0" prefHeight="200.0" prefWidth="150.0">
        <columns>
          <TableColumn fx:id="identifiant" prefWidth="75.0" text="identifiant" />
          <TableColumn fx:id="score" prefWidth="75.0" text="score" />
        </columns>
      </TableView>
      <Button fx:id="retour" layoutX="325.0" layoutY="404.0" mnemonicParsing="false" onAction="#clickOnRetour" prefHeight="20.0" prefWidth="100.0" text="RETOUR" />
      <Button fx:id="quitter" layoutX="325.0" layoutY="466.0" mnemonicParsing="false" onAction="#clickOnExit" prefHeight="20.0" prefWidth="100.0" text="QUITTER" />
   </children>
</AnchorPane>
